<div class="row">
  <table class="dataTable table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>UserName</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users && @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
