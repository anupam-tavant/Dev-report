<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Users</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            List of Users
        </div>
        <!-- /.panel-heading -->
        <div class="panel-body panel30">
          <div class="row">
            <table class="dataTable table table-striped table-bordered" cellspacing="0" width="100%">
              <thead>
                <tr>
                  <th>UserName</th>
                  <th>Email</th>
                </tr>
              </thead>
              <tbody>
                <% @users && @users.each do |user| %>
                  <tr>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
          </div>
    </div>
  </div>
</div>
